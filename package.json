{
  "name": "rally-clientmetrics",
  "version": "5.0.1",
  "description": "Metrics aggregation for Rally Software",
  "main": "builds/rallymetrics.js",
  "scripts": {
    "lint": "eslint src",
    "docs": "grunt jsdoc",
    "test": "npm run webpack:test && grunt test",
    "webpack": "webpack --progress --config webpack.dev.config.js && webpack --progress --config webpack.prod.config.js && webpack --progress --config webpack.sdk.config.js",
    "webpack:test": "webpack --debug --progress --config webpack.test.config.js",
    "webpack:analyze": "webpack --debug --progress --config webpack.dev.config.js --json > webpack-stats.json",
    "prepublish": "npm run webpack && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RallySoftware/clientmetrics.git"
  },
  "author": "Rally Software",
  "contributors": [
    {
      "name": "Matt Parrish",
      "email": "matt.r.parrish@gmail.com"
    },
    {
      "name": "Matt Greer",
      "email": "matt.e.greer@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RallySoftware/clientmetrics/issues"
  },
  "files": [
    "builds",
    "doc"
  ],
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-1": "6.22.0",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-config-rally": "4.0.0",
    "eslint-plugin-jasmine": "1.8.1",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-rally": "1.2.5",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-whitespace": "^0.1.3",
    "express": "~3.3.4",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-express": "~1.0.1",
    "grunt-jsdoc": "2.1.0",
    "grunt-mocha": "1.0.2",
    "grunt-open": "~0.2.3",
    "grunt-release": "0.14.0",
    "grunt-text-replace": "^0.4.0",
    "mocha": "2.5.3",
    "node-static": "^0.7.6",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "uuid": "2.0.2",
    "webpack": "1.13.1",
    "when": "3.7.7"
  },
  "dependencies": {
    "uuid": "^2.0.1",
    "when": "~2.7.0"
  },
  "publishConfig": {
    "registry": "http://npm-publish.f4tech.com"
  }
}
