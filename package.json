{
  "name": "rally-clientmetrics",
  "version": "6.0.0",
  "description": "Metrics aggregation for Rally Software",
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint src",
    "docs": "grunt jsdoc",
    "test": "jest src",
    "build": "webpack --progress --config webpack.dev.config.js && webpack --progress --config webpack.prod.config.js && webpack --progress --config webpack.sdk.config.js",
    "build:test": "webpack --debug --progress --config webpack.test.config.js",
    "webpack:analyze": "webpack --debug --progress --config webpack.dev.config.js --json > webpack-stats.json",
    "prepublish": "npm run build && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RallySoftware/clientmetrics.git"
  },
  "author": "Rally Software",
  "contributors": [
    {
      "name": "Matt Parrish",
      "email": "matt.r.parrish@gmail.com"
    },
    {
      "name": "Matt Greer",
      "email": "matt.e.greer@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RallySoftware/clientmetrics/issues"
  },
  "files": [
    "builds",
    "doc"
  ],
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.3.2",
    "babel-preset-stage-2": "6.22.0",
    "eslint": "3.19.0",
    "eslint-config-rally": "4.0.0",
    "eslint-plugin-jasmine": "2.2.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-rally": "1.2.5",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-whitespace": "^0.1.3",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-jsdoc": "2.1.0",
    "grunt-release": "0.14.0",
    "jest": "19.0.2",
    "sinon": "2.1.0",
    "webpack": "2.3.2"
  },
  "dependencies": {
    "uuid": "3.0.1"
  },
  "publishConfig": {
    "registry": "http://npm-publish.f4tech.com"
  }
}
