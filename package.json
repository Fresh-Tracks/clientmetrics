{
  "name": "rally-clientmetrics",
  "version": "4.2.0",
  "description": "Metrics aggregation for Rally Software",
  "main": "builds/rallymetrics.js",
  "scripts": {
    "lint": "eslint src",
    "docs": "grunt jsdoc",
    "test": "npm run webpack:test && grunt test",
    "webpack": "webpack --progress --config webpack.dev.config.js && webpack --progress --config webpack.prod.config.js",
    "webpack:test": "webpack --debug --progress --config webpack.test.config.js",
    "webpack:analyze": "webpack --debug --progress --config webpack.dev.config.js --json > webpack-stats.json",
    "prepublish": "npm run webpack && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RallySoftware/clientmetrics.git"
  },
  "author": "Rally Software",
  "contributors": [
    {
      "name": "Matt Parrish",
      "email": "matt.r.parrish@gmail.com"
    },
    {
      "name": "Matt Greer",
      "email": "matt.e.greer@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RallySoftware/clientmetrics/issues"
  },
  "files": [
    "builds",
    "doc"
  ],
  "devDependencies": {
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-1": "6.5.0",
    "chai": "~1.9.0",
    "eslint": "^1.10.3",
    "eslint-config-rally": "2.3.0",
    "eslint-plugin-jasmine": "1.8.1",
    "eslint-plugin-rally": "1.1.0",
    "eslint-plugin-react": "5.1.1",
    "eslint-plugin-whitespace": "^0.1.3",
    "express": "~3.3.4",
    "grunt": "~0.4.2",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-express": "~1.0.1",
    "grunt-jsdoc": "^1.1.0",
    "grunt-mocha": "^0.4.15",
    "grunt-open": "~0.2.3",
    "grunt-release": "^0.13.0",
    "grunt-text-replace": "^0.4.0",
    "mocha": "2.4.5",
    "node-static": "^0.7.6",
    "sinon": "~1.9.0",
    "sinon-chai": "2.8.0",
    "uuid": "2.0.2",
    "webpack": "1.13.0"
  },
